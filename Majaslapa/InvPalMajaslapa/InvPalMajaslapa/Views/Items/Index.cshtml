@model IEnumerable<Items>
@{
    ViewData["Title"] = "Preces";
}

<h1>Preces</h1>
<a asp-controller="Items" asp-action="Create" asp-route-id="@ViewBag.WarehouseId" class="btn btn-primary">
    <i class="bi bi-plus-lg"></i> Jauna Prece
</a>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Nosaukums</th>
            <th scope="col">Apraksts</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="35%">@obj.Name</td>
                <td width="65%">@obj.Description</td>
            </tr>
        }
    </tbody>
</table>